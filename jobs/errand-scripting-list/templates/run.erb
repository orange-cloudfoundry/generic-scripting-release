#!/bin/bash

set -e # exit immediately if a simple command exits with a non-zero status
set -u # report the usage of uninitialized variables

<% if_p('scripting.errand-script-list') do |value| %>
<% p('scripting.errand-script-list').each do |script| %>
<%= script %>
<% end %>
<% end %>


EXITSTATUS=0

echo "errand run script list is complete; exit status $EXITSTATUS"
exit $EXITSTATUS
