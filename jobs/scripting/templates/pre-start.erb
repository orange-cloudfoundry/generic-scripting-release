#!/bin/bash

set -e # exit immediately if a simple command exits with a non-zero status
set -u # report the usage of uninitialized variables


<% if_p('scripting.pre-start-script') do |script| %>
<%= p('scripting.pre-start-script') %>
<% end %>




EXITSTATUS=0

echo " pre-start is complete; exit status $EXITSTATUS"
exit $EXITSTATUS
